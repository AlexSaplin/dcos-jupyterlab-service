{
  "id": "/dev/beakerx/notebook",
  "instances": 1,
  "cpus": 2,
  "mem": 8192,
  "user": "nobody",
  "cmd": "/usr/local/bin/start.sh ${CONDA_DIR}/bin/jupyter lab --notebook-dir=\"/mnt/mesos/sandbox\"",
  "container": {
    "type": "MESOS",
    "docker": {
      "image": "vishnumohan/beakerx-dcos:tfos",
      "forcePullImage": true
    }
  },
  "secrets": {
    "awsAccessKeyId": {
      "source": "dev/AWS_ACCESS_KEY_ID"
    },
    "awsSecretAccessKey": {
      "source": "dev/AWS_SECRET_ACCESS_KEY"
    },
    "serviceCredential": {
      "source": "dev/beakerx/serviceCredential"
    }
  },
  "env": {
    "AWS_ACCESS_KEY_ID": {
      "secret": "awsAccessKeyId"
    },
    "AWS_SECRET_ACCESS_KEY": {
      "secret": "awsSecretAccessKey"
    },
    "DCOS_SERVICE_ACCOUNT_CREDENTIAL": {
      "secret": "serviceCredential"
    },
    "AWS_REGION": "us-east-1",
    "CONDA_ENVS_PATH": "/mnt/mesos/sandbox/conda/envs:/opt/conda/envs",
    "CONDA_PKGS_DIRS": "/mnt/mesos/sandbox/conda/pkgs:/opt/conda/pkgs",
    "HOME": "/mnt/mesos/sandbox",
    "JUPYTER_RUNTIME_DIR": "/mnt/mesos/sandbox/.local/share/jupyter/runtime",
    "S3_ENDPOINT": "s3.us-east-1.amazonaws.com",
    "S3_USE_HTTPS": "1",
    "S3_VERIFY_SSL": "1",
    "SPARK_MASTER_URL": "mesos://zk://zk-1.zk:2181,zk-2.zk:2181,zk-3.zk:2181,zk-4.zk:2181,zk-5.zk:2181/mesos",
    "SPARK_DRIVER_CORES": "2",
    "SPARK_DRIVER_MEMORY": "4g",
    "SPARK_CONF_CORES_MAX": "spark.cores.max=10",
    "SPARK_CONF_EXECUTOR_CORES": "spark.executor.cores=2",
    "SPARK_CONF_EXECUTOR_MEMORY": "spark.executor.memory=4g",
    "SPARK_CONF_MESOS_EXECUTOR_HOME": "spark.mesos.executor.home=/opt/spark",
    "SPARK_CONF_MESOS_CONTAINERIZER": "spark.mesos.containerizer=mesos",
    "SPARK_CONF_MESOS_PRINCIPAL": "spark.mesos.principal=dev_beakerx",
    "SPARK_CONF_MESOS_ROLE": "spark.mesos.role=dev-beakerx",
    "SPARK_CONF_MESOS_EXECUTOR_DOCKER_IMAGE":"spark.mesos.executor.docker.image=vishnumohan/spark-dcos:tfos",
    "SPARK_CONF_MESOS_DRIVER_LABELS": "spark.mesos.driver.labels=DCOS_SPACE:/dev/beakerx",
    "SPARK_CONF_MESOS_TASK_LABELS": "spark.mesos.task.labels=DCOS_SPACE:/dev/beakerx",
    "SPARK_CONF_MESOS_DRIVER_SECRET_NAMES": "spark.mesos.driver.secret.names=dev/AWS_ACCESS_KEY_ID,dev/AWS_SECRET_ACCESS_KEY",
    "SPARK_CONF_MESOS_DRIVER_SECRET_ENVKEYS": "spark.mesos.driver.secret.envkeys=AWS_ACCESS_KEY_ID,AWS_SECRET_ACCESS_KEY",
    "SPARK_CONF_MESOS_EXECUTOR_SECRET_NAMES": "spark.mesos.executor.secret.names=dev/AWS_ACCESS_KEY_ID,dev/AWS_SECRET_ACCESS_KEY",
    "SPARK_CONF_MESOS_EXECUTOR_SECRET_ENVKEYS": "spark.mesos.executor.secret.envkeys=AWS_ACCESS_KEY_ID,AWS_SECRET_ACCESS_KEY",
    "SPARK_CONF_EVENTLOG_ENABLED": "spark.eventLog.enbled=true",
    "SPARK_CONF_EVENTLOG_DIR": "spark.eventLog.dir=s3a://vishnu-mohan/spark/history",
    "SPARK_MESOS_KRB5_CONF_BASE64": "dmlzaG51Cg==",
    "TERM": "xterm-256color"
  },
  "labels": {
    "MARATHON_SINGLE_INSTANCE_APP": "true",
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_ENABLED": "true",
    "HAPROXY_0_REDIRECT_TO_HTTPS": "true",
    "HAPROXY_0_VHOST": "vhost.example.com",
    "HAPROXY_0_PATH": "/service/dev/beakerx/notebook",
    "HAPROXY_2_ENABLED": "true",
    "HAPROXY_2_REDIRECT_TO_HTTPS": "true",
    "HAPROXY_2_VHOST": "vhost.example.com",
    "HAPROXY_2_PATH": "/service/dev/beakerx/spark",
    "HAPROXY_2_HTTP_BACKEND_PROXYPASS_PATH": "/service/dev/beakerx/spark"
  },
  "portDefinitions": [
    {
      "port": 0,
      "protocol": "tcp",
      "name": "notebook",
      "labels": {
        "VIP_0": "/dev/beakerx/notebook:8888"
      }
    },
    {
      "port": 0,
      "protocol": "tcp",
      "name": "sparkdriver",
      "labels": {
        "VIP_1": "/dev/beakerx/sparkdriver:7077"
      }
    },
    {
      "port": 0,
      "protocol": "tcp",
      "name": "sparkui",
      "labels": {
        "VIP_2": "/dev/beakerx/sparkui:4040"
      }
    },
    {
      "port": 0,
      "protocol": "tcp",
      "name": "sparkblockmanager"
    }
  ],
  "healthChecks": [
    {
      "portIndex": 0,
      "protocol": "MESOS_HTTP",
      "path": "/service/dev/beakerx/notebook/api",
      "gracePeriodSeconds": 30,
      "intervalSeconds": 20,
      "timeoutSeconds": 10,
      "maxConsecutiveFailures": 3
    }
  ],
  "upgradeStrategy": {
    "minimumHealthCapacity": 0,
    "maximumOverCapacity": 0
  },
  "unreachableStrategy": {
    "expungeAfterSeconds": 0,
    "inactiveAfterSeconds": 0
  }
}
